{
  "name": "server",
  "projectType": "application",
  "sourceRoot": "apps/server",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "apps/server"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
        "cwd": "apps/server"
      }
    }
  }
}